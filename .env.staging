# ========================================
# AIS Aviation System - Staging Environment
# ========================================

# App Configuration
VITE_APP_ID=ais-aviation-system
NODE_ENV=staging

# Database Configuration
DATABASE_URL=mysql://staging_user:CHANGE_ME@staging-db-host:3306/ais_aviation_staging

# Authentication & Security
JWT_SECRET=CHANGE_ME_TO_A_SECURE_32_CHAR_MINIMUM_SECRET
OAUTH_SERVER_URL=https://oauth.manus.space
OWNER_OPEN_ID=staging-owner-id

# Built-in Forge API (Manus AI)
BUILT_IN_FORGE_API_URL=https://api.manus.space
BUILT_IN_FORGE_API_KEY=CHANGE_ME_STAGING_API_KEY

# Payment Gateway - Stripe (Test Mode for staging)
STRIPE_SECRET_KEY=sk_test_CHANGE_ME
STRIPE_WEBHOOK_SECRET=whsec_CHANGE_ME
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_CHANGE_ME

# Email Service
EMAIL_HOST=smtp.sendgrid.net
EMAIL_PORT=587
EMAIL_USER=apikey
EMAIL_PASSWORD=CHANGE_ME_SENDGRID_API_KEY
EMAIL_FROM=AIS Aviation Staging <staging@ais.com>

# AI Features
OPENAI_API_KEY=sk-CHANGE_ME

# AWS S3 Storage
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=CHANGE_ME
AWS_SECRET_ACCESS_KEY=CHANGE_ME
AWS_S3_BUCKET=ais-aviation-staging

# Application Settings
PORT=3000
FRONTEND_URL=https://staging.ais-aviation-system.com

# Session cookie settings (staging)
COOKIE_DOMAIN=.ais-aviation-system.com
COOKIE_SECURE=true
COOKIE_SAME_SITE=strict

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=900000

# Feature Flags
ENABLE_AI_CHAT=true
ENABLE_LOYALTY_PROGRAM=true
ENABLE_MULTI_CURRENCY=true
ENABLE_PWA=true

# Logging & Monitoring
LOG_LEVEL=info
LOG_PRETTY=false

# Development Tools
DEBUG=false
ENABLE_QUERY_LOGGING=false
