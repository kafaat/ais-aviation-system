    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@vitejs/plugin-react": "^5.0.4",
