    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "uuid": "^13.0.0",
    "vite": "^7.1.7",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-manus-runtime": "^0.0.56",
    "vite-plugin-pwa": "^1.1.0",
    "vitest": "^2.1.4"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "overrides": {
      "tailwindcss>nanoid": "3.3.7"
    }
  },
  "size-limit": [
    {
      "name": "Main JS Bundle",
      "path": "dist/public/assets/index-*.js",
      "limit": "500 KB",
      "gzip": true
    },
    {
      "name": "Vendor Chunk",
      "path": "dist/public/assets/vendor-*.js",
      "limit": "300 KB",
      "gzip": true
    },
    {
      "name": "UI Components Chunk",
      "path": "dist/public/assets/ui-*.js",
      "limit": "150 KB",
      "gzip": true
    },
    {
      "name": "Charts Chunk",
      "path": "dist/public/assets/charts-*.js",
      "limit": "200 KB",
      "gzip": true
    },
    {
      "name": "Total JS",
      "path": "dist/public/assets/*.js",
      "limit": "1.2 MB",
      "gzip": true
    },
    {
      "name": "Total CSS",
      "path": "dist/public/assets/*.css",
      "limit": "100 KB",
      "gzip": true
    }
  ]
}
